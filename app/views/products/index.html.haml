- if dark_side?
  - metatags :title => t(:index_title, :model => Product.model_name.human)
  .action_links
    = link_to t(:new_link, :model => Product.model_name.human), new_product_path
  .products
    = render @products
- else
  - metatags :title => "Les produits #{@partner.commercial_name}", :baseline => @partner.baseline

  .products
    = render @products

